{
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    "**/*.css",
    "**/*.scss",
    "**/*.html",
    "**/package.json",
    "**/*.md",
    "**/LICENSE"
  ],
  "exclude": [
    "**/node_modules/**/*",
    "**/dist/**/*",
    "**/build/**/*",
    "**/out/**/*",
    "**/.next/**/*",
    "**/.nuxt/**/*",
    "**/.yarn/**/*",
    "@types",
    "**/*.min.css",
    "**/.storybook/**/*",
    ".github/**/*"
  ]
}